<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/mvc
        http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd">

  <import resource="../init.xml" />
  <mvc:annotation-driven validator="validator" />
  <!-- we do not use JSR 303 validator annotations, so disable validation -->
  <bean id="validator" class="org.esupportail.smsu.web.NoValidation" />

  <bean id="WsController" class="org.esupportail.smsu.web.ws.WsController">
		<property name="authorizedClientNames" value="${smsu.ws.authorizedClientNames}"></property>
  </bean>
</beans>
