<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<!-- 
	/properties/jsf/navigation-rules.xml
	The configuration file of Java Server Faces that gives 
	the navigation rules between views.
	This file is referenced by /webapp/WEB-INF/web.xml. 
	It should not be modified but by the developers. 
-->

<faces-config>

	<navigation-rule>
		<display-name>index.jsp -> welcome.jsp</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administrators</display-name>
		<from-view-id>/stylesheets/administrators.jsp</from-view-id>
		<navigation-case>
			<from-outcome>addAdmin</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteAdmin</from-outcome>
			<to-view-id>
				/stylesheets/administratorDelete.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administratorDelete</display-name>
		<from-view-id>
			/stylesheets/administratorDelete.jsp
		</from-view-id>
		<navigation-case>
			<from-outcome>adminDeleted</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administratorAdd</display-name>
		<from-view-id>/stylesheets/administratorAdd.jsp</from-view-id>
		<navigation-case>
			<from-outcome>adminAdded</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>ldapSearch</display-name>
		<from-view-id>/stylesheets/ldapSearch.jsp</from-view-id>
		<navigation-case>
			<from-outcome>cancelToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelToDepartmentManagerAdd</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerAdd.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>ldapResults</display-name>
		<from-view-id>/stylesheets/ldapResults.jsp</from-view-id>
		<navigation-case>
			<from-outcome>userSelectedToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/stylesheets/ldapSearch.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>performSendSMS</display-name>
		<from-view-id>/stylesheets/sendSMS.jsp</from-view-id>
		<navigation-case>
			<from-outcome>envoiOK</from-outcome>
			<to-view-id>/stylesheets/sendResult.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<navigation-case>
			<from-outcome>applicationRestarted</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationWelcome</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationAdministrators</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationPreferences</from-outcome>
			<to-view-id>/stylesheets/preferences.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationAbout</from-outcome>
			<to-view-id>/stylesheets/about.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>casLogin</from-outcome>
			<to-view-id>/stylesheets/cas.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ldapSearch</from-outcome>
			<to-view-id>/stylesheets/ldapSearch.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ldapChoose</from-outcome>
			<to-view-id>/stylesheets/ldapResults.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationMembership</from-outcome>
			<to-view-id>/stylesheets/membership.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationMembershipNotInLDAP</from-outcome>
			<to-view-id>/stylesheets/membershipNotInLdap.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationSendSMS</from-outcome>
			<to-view-id>/stylesheets/sendSMS.jsp</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>cancelSend</from-outcome>
			<to-view-id>/stylesheets/sendTRACK/search_sms.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>TrackingSend</from-outcome>
			<to-view-id>/stylesheets/sendTRACK/search_sms.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>detailSend</from-outcome>
			<to-view-id>/stylesheets/sendTRACK/detail_sms.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationMembership</from-outcome>
			<to-view-id>/stylesheets/membership.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationSendSMS</from-outcome>
			<to-view-id>/stylesheets/sendSMS.jsp</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>navigationApproveSMS</from-outcome>
			<to-view-id>/stylesheets/approvalSMS/list_sms.jsp</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>navigationManageTemplates</from-outcome>
			<to-view-id>/stylesheets/list_templates.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationCreateTemplate</from-outcome>
			<to-view-id>/stylesheets/create_modify_templates.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationModifyTemplate</from-outcome>
			<to-view-id>/stylesheets/create_modify_templates.jsp</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>navigationAdminRoles</from-outcome>
			<to-view-id>/stylesheets/adminROLE/list_role.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationDetailRole</from-outcome>
			<to-view-id>/stylesheets/adminROLE/detail_role.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationCreateRole</from-outcome>
			<to-view-id>/stylesheets/adminROLE/create_role.jsp</to-view-id>
		</navigation-case>

	   <navigation-case>
			<from-outcome>navigationAdminGroups</from-outcome>
			<to-view-id>/stylesheets/adminGROUP/list_group.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationDetailGroup</from-outcome>
			<to-view-id>/stylesheets/adminGROUP/detail_group.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationCreateGroup</from-outcome>
			<to-view-id>/stylesheets/adminGROUP/create_group.jsp</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>navigationAdminService</from-outcome>
			<to-view-id>/stylesheets/list_services.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationCreateService</from-outcome>
			<to-view-id>/stylesheets/create_modify_services.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationModifyService</from-outcome>
			<to-view-id>/stylesheets/create_modify_services.jsp</to-view-id>
		</navigation-case>
		
	</navigation-rule>
</faces-config>
